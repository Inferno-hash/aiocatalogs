{
  "name": "aiocatalogs",
  "version": "1.0.0",
  "description": "All-in-one catalogs addon for Stremio",
  "main": "dist/addon/server.js",
  "scripts": {
    "start": "npm run build && node dist/addon/server.js",
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only packages/addon/server.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "release": "bumpp"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/panteLx/aiocatalogs.git"
  },
  "keywords": [
    "stremio",
    "addon",
    "catalogs"
  ],
  "author": "panteLx",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/panteLx/aiocatalogs/issues"
  },
  "homepage": "https://github.com/panteLx/aiocatalogs#readme",
  "dependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/express": "^5.0.1",
    "@types/node": "^22.15.15",
    "@types/node-fetch": "^2.6.12",
    "body-parser": "^2.2.0",
    "express": "^5.1.0",
    "node-fetch": "^2.7.0",
    "stremio-addon-sdk": "^1.6.10",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3",
    "bumpp": "^10.1.0",
    "prettier": "^3.3.2",
    "vitest": "^3.1.3",
    "@vitest/coverage-v8": "^3.1.3"
  }
}
